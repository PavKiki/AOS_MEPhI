{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Build lab1",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/src/main.c",
                "${workspaceFolder}/src/firstTask/firstTask.c",
                "${workspaceFolder}/src/secondTask/secondTask.c",
                "${workspaceFolder}/src/fourthTask/fourthTask.c",
                "${workspaceFolder}/src/sixthTask/sixthTask.c",
                "${workspaceFolder}/src/seventhTask/seventhTask.c",
                "${workspaceFolder}/src/eighthTask/eighthTask.c",
                "${workspaceFolder}/src/ninethTask/ninethTask.c",
                "${workspaceFolder}/src/tenthTask/tenthTask.c",
                "${workspaceFolder}/src/eleventhTask/eleventhTask.c",
                "-o",
                "${workspaceFolder}/lab1",
                "-pedantic-errors"
            ],
            "options": {
                "cwd": "${workspaceFolder}/resources"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "shell",
            "label": "Remove executable",
            "command": "rm",
            "args": [
                "-rf",
                "${workspaceFolder}/lab1"
            ]
        }
    ],
    "version": "2.0.0"
}